{
    "name": "manuelgil/payu-integration-sdk",
    "description": "Integration of PayU Latam Payments PHP SDK",
    "type": "project",
    "license": "MIT",
    "support": {
        "wiki": "http://developers.payulatam.com/en/sdk/sandbox.html"
    },
    "authors": [
        {
            "name": "Manuel Gil",
            "email": "atencion.ms@outlook.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.2.1",
        "vlucas/phpdotenv": "^2.5",
        "phroute/phroute": "^2.1",
        "monolog/monolog": "^1.24"
	},
    "autoload": {
        "psr-4": {
            "App\\":"app/"
        },
        "files": [
            "app/config/Config.php",
            "app/lib/PayU.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "5.* || 7.*"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
